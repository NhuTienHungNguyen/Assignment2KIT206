<UserControl x:Class="NewAssignment2KIT206.FilteringByLevelControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NewAssignment2KIT206"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ComboBox HorizontalAlignment="Left" Margin="10,27,0,0" VerticalAlignment="Top" Width="133"
                  ItemsSource="{Binding Source={StaticResource positionValues}}"
                  SelectedIndex="0" SelectionChanged="ComboBox_SelectionChanged"  />
        <Label Content="Filter by Level" HorizontalAlignment="Left" Margin="10,1,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
